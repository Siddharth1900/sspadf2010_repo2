		{
			"name": "[concat(parameters('factoryName'), '/emp_grade')]",
			"type": "Microsoft.DataFactory/factories/dataflows",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "employee_grade",
				"type": "UDFLibrary",
				"typeProperties": {
					"sources": [],
					"sinks": [],
					"transformations": [],
					"scriptLines": [
						"emp_grade(short) as string = iif(i1>=1500 && i1<=2500, 'Grade-B', 'Grade-A')"
					]
				}
			},
			"dependsOn": []
		},
		{
